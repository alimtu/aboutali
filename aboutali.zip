    <script>
        $(document).ready(function () {
            let errorMsg = document.querySelector("#error-msg");
            let validMsg = document.querySelector("#valid-msg");
            let errorMap = ["شماره موبایل نامعتبر است.", "کد کشور اشتباه است.", "طول شماره موبایل اشتباه است.", "طول شماره موبایل اشتباه است.", "شماره موبایل معتبر نیست."];

            let input = document.querySelector("#phone , #id_phone");
            if (input) {
                window.iti = window.intlTelInput(input, {
                    initialCountry: "auto",
                    separateDialCode: true,
                    preferredCountries: ['ir', 'tr'],
                    hiddenInput: "full_phone",
                    geoIpLookup: function (callback) {
                        $.get('https://ipinfo.io', function () {
                        }, "jsonp").always(function (resp) {
                            var countryCode = (resp && resp.country) ? resp.country : "ir";
                            callback(countryCode);
                        });
                    },
                    utilsScript: "/static/assets/Base/js/utils.js" // just for formatting/placeholders etc
                });

            }


            var reset = function () {
                if (input) {
                    input.classList.remove("error");
                }
                errorMsg.innerHTML = "";
                errorMsg.classList.add("d-none");
                if (validMsg) {
                    validMsg.classList.add("d-none");
                }
            };
            if (input) {
                // on blur: validate
                input.addEventListener('blur', function () {
                    reset();
                    if (input.value.trim()) {
                        if (window.iti.isValidNumber()) {
                            
                        } else {
                            input.classList.add("error");
                            var errorCode = window.iti.getValidationError();
                            errorMsg.innerHTML = errorMap[errorCode];
                            errorMsg.classList.remove("d-none");
                        }
                    }
                });

                // on keyup / change flag: reset
                input.addEventListener('change', reset);
                input.addEventListener('keyup', reset);
            }
        });


    </script>
    


